<?xml version="1.0" encoding="UTF-8"?>

<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="AdminLsItemGridMainActionsSection">
        <element name="reset" type="button" selector="//*[contains(@class, 'admin__data-grid-header')]//span[contains(@class,'action-menu-item') and text()='Reset']"/>
        <element name="ok" type="button" selector="//button[@data-role='action']//span[text()='OK']"/>
        <element name="actions" type="text" selector=".admin__data-grid-header-row .action-select"/>
        <element name="deletedSuccessMessage" type="button" selector="//*[@class='message message-success success']"/>
    </section>
</sections>
