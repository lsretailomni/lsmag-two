<?xml version="1.0" encoding="UTF-8"?>

<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <actionGroup name="LsNavigateToAdminCustomerActionGroup">
        <annotations>
            <description>Goes to the Magento admin 'Customer' page.</description>
        </annotations>

        <amOnPage url="{{AdminCustomerPage.url}}" stepKey="navigateToCustomerPage"/>
        <waitForPageLoad stepKey="waitForPageLoad1"/>
    </actionGroup>
</actionGroups>
