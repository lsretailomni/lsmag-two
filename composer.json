{
    "name": "lsretail/lsmag-two",
    "description": "LS Ecommerce - Magento Integration",
    "type": "magento2-module",
    "license": "proprietary",
    "version": "2.0.0",
    "authors": [
        {
            "name": "LS Retail - Ecommerce Team",
            "email": "lsomnikl@lsretail.com"
        }
    ],
    "require": {
        "myclabs/php-enum": "^1.5",
        "sabre/xml": "^1.5",
        "nabil1337/case-helper": "^0.1.0",
        "ramsey/uuid": "*",
        "nesbot/carbon": "*"
    },
    "suggest": {
        "kiwicommerce/module-cron-scheduler": "^1.0.5"
    },
    "autoload": {
        "psr-4": {
            "Ls\\Core\\": "src/Core/",
            "Ls\\Omni\\": "src/Omni/",
            "Ls\\OmniGraphQl\\": "src/OmniGraphQl/",
            "Ls\\Replication\\": "src/Replication/",
            "Ls\\Customer\\": "src/Customer/",
            "Ls\\CustomerGraphQl\\": "src/CustomerGraphQl/",
            "Ls\\Webhooks\\": "src/Webhooks/",
            "Ls\\CommerceCloud\\": "src/CommerceCloud/"
        },
        "files": [
            "src/Core/registration.php",
            "src/Omni/registration.php",
            "src/OmniGraphQl/registration.php",
            "src/Replication/registration.php",
            "src/Customer/registration.php",
            "src/CustomerGraphQl/registration.php",
            "src/Webhooks/registration.php",
            "src/CommerceCloud/registration.php"
